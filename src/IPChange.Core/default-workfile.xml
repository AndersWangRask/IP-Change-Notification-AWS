<?xml version="1.0" encoding="utf-8" ?>
<ClientToAWS-config>
  <BaseSettings>
    <AWSAccessKeyID></AWSAccessKeyID>
    <AWSSecretAccessKey></AWSSecretAccessKey>
    <AWSRegion></AWSRegion>
  </BaseSettings>
  <multiClientState>
    <clientName></clientName>
    <Route53>
      <R53ZoneId></R53ZoneId>
      <name></name>
      <useEncryption></useEncryption>
      <encryptionPassword></encryptionPassword>
    </Route53>
  </multiClientState>
  <Route53>
    <host>
      <R53ZoneId></R53ZoneId>
      <name ttl="60"></name>
    </host>
  </Route53>
  <EC2SecurityGroup>
    <entry>
      <!-- RDP -->
      <!-- THE GROUP ID of the Security Group -->
      <groupId></groupId>
      <portRange>3389</portRange>
      <ipProtocol>tcp</ipProtocol>
      <description>Remote Desktop Protocol optional description</description> 
    </entry>
    <entry>
      <!-- MS SQL -->
      <groupId></groupId>
      <portRange>1433</portRange>
      <ipProtocol>tcp,icmp,udp</ipProtocol>
    </entry>
    <entry>
      <!-- PowerShell -->
      <groupId></groupId>
      <portRange>5985-5986</portRange>
    </entry>
  </EC2SecurityGroup>
  <Notification>
    <emailConfig>
      <smtpServer>smtp.gmail.com</smtpServer>
      <smtpServerPort>587</smtpServerPort>
      <username></username>
      <password></password>
      <domain></domain>
      <fromEmail></fromEmail>
      <subjectPrefix></subjectPrefix>
      <optionalMessage></optionalMessage>
    </emailConfig>
    <recipients>
      <email type="summary">123@email.com</email>
      <email type="full">456@email.com</email>
      <email type="full" display="Mr John Doe">jd@email.com</email>
    </recipients>
  </Notification>
</ClientToAWS-config>